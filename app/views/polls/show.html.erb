<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @poll.title %>
</p>
<p>
  Start:
  <%= @poll.timeframe_start%>
</p>
<p>
  End:
  <%= @poll.timeframe_end%>
</p>
<p>
  Optimal Times:
  <ol>
    <% @optimal_times.each do |time| %>
      <li><%= time %></li>
    <% end %>
  </ol>
</p>

<p>
  Poll start time:
  <%= @poll.daily_start%>
</p>
<p>
  Poll end time:
  <%= @poll.daily_end%>
</p>

<h2>Users</h2>
<%= render @poll.users %>

<h2>Add user:</h2>
<%= render "users/form" %>

<h2>Comments:</h2>
<%= render @poll.comments %>

<%= link_to 'Edit', edit_poll_path(@poll) %> |
<%= link_to 'Back', polls_path %>
